<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Hotwire for Frontend Developers</title>
  <script src="/hotwire/javascript/turbo.es2017-esm.js" data-turbo-track="reload" type="module"></script>
  <script data-turbo-track="reload" type="module">
    import {Application} from "/hotwire/javascript/stimulus.js"
    import PopupController from "/hotwire/javascript/popup_controller.js"
    import FormlikeController from "/hotwire/javascript/formlike_controller.js"
    import ModalController from "/hotwire/javascript/modal_controller.js"

    window.Stimulus = Application.start()

    Stimulus.register("popup", PopupController)
    Stimulus.register("formlike", FormlikeController)
    Stimulus.register("modal", ModalController)
  </script>
  <link rel="stylesheet" href="/hotwire/styles/output.css" data-turbo-track="reload">
</head>
<body>
<div class="bg-white flex justify-between p-1 items-start">
  <a href="/" class="text-xl p-1 text-orange-600">
    Top</a>
  <a href="/" class="text-xl p-1 font-bold text-gray-400 hover:text-orange-600">Hotwire for Frontend Developers</a>
  <div class="text-lg p-2 rounded bg-yellow-400 text-black">Hotwire</div>
</div>
<div class="text-2xl fixed opacity-70 right-0 top-0 w-56 bg-rose-600 text-white font-bold text-center text-white rotate-45 translate-x-12 translate-y-12">
  beta
</div>

